{
  "name": "nisshi-dev-survey",
  "version": "1.2.1",
  "type": "module",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "24.x",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "npm",
      "version": "11.x",
      "onFail": "warn"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:client && vite build",
    "preview": "vite preview",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "generate:client": "orval",
    "clean": "rm -r dist node_modules/.cache"
  },
  "dependencies": {
    "@heroui/react": "3.0.0-beta.6",
    "@heroui/styles": "3.0.0-beta.6",
    "@tailwindcss/typography": "0.5.19",
    "@vercel/analytics": "^1.6.1",
    "motion": "12.34.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "10.1.0",
    "react-router-dom": "7.13.0",
    "recharts": "3.7.0",
    "remark-gfm": "4.0.1",
    "swr": "2.4.0",
    "tailwind-variants": "3.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@orval/swr": "8.4.0",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.x",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/coverage-v8": "4.0.18",
    "agentation": "^2.2.1",
    "globals": "17.3.0",
    "jsdom": "28.1.0",
    "orval": "8.4.0",
    "tsx": "4.x",
    "typescript": "5.x",
    "ultracite": "7.2.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  }
}
